ableToDrink = True 
if cafe == "Octane":
    if balance < 6:
        ableToDrink = False
if cafe == "Galloway":
    if balance < 5:
        ableToDrink = False
if cafe == "Starbucks":
    if balance < 4:
        ableToDrink = False
if cafe == "Revelator":
    if balance < 3:
        ableToDrink = False
if cafe == "Dunkin":
    if balance < 2:
        ableToDrink = False

if ableToDrink:
    print("With", str(balance), "dollars, I can buy coffee at", cafe)
else:
    print("With", str(balance), "dollars, I cannot buy coffee at", cafe)

